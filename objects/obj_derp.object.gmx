    case playerstate.hit:
    y += fallspeed;
    sprite_index = spr_derphit
    if hitplayed == false
    {
        image_blend = c_red;
        audio_play_sound(snd_hurt,0,false);
        alarm[0] = 30;
        obj_points.points -= 50;
        hitplayed = true;
